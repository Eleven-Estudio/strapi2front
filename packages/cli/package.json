{
  "name": "strapi2front",
  "version": "0.5.0",
  "description": "Generate TypeScript types, services, and framework actions from your Strapi schema",
  "type": "module",
  "bin": {
    "strapi2front": "./dist/bin/strapi2front.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "start": "node dist/bin/strapi2front.js"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@strapi2front/client": "workspace:*",
    "@strapi2front/core": "workspace:*",
    "@strapi2front/generators": "workspace:*",
    "commander": "^12.0.0",
    "execa": "^8.0.0",
    "fast-glob": "^3.3.0",
    "ora": "^8.0.0",
    "picocolors": "^1.0.0",
    "pkg-types": "^1.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "strapi",
    "cms",
    "astro",
    "nextjs",
    "typescript",
    "codegen",
    "types",
    "actions"
  ],
  "license": "MIT",
  "author": "Eleven Estudio (https://github.com/elevenestudio)",
  "homepage": "https://strapi2front.elevenestudio.com",
  "contributors": [
    {
      "name": "Geovanny Gil",
      "email": "esgeovannygil@gmail.com",
      "url": "https://github.com/GeovannyGil"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eleven-estudio/strapi2front.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/eleven-estudio/strapi2front/issues"
  }
}
