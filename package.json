{
  "name": "strapi2front-monorepo",
  "version": "0.1.0",
  "description": "Generate TypeScript types, services, and framework actions from your Strapi schema",
  "type": "module",
  "homepage": "https://strapi2front.dev",
  "contributors": [
    {
      "name": "Geovanny Gil",
      "email": "esgeovannygil@gmail.com",
      "url": "https://github.com/GeovannyGil"
    }
  ],
  "keywords": [
    "strapi",
    "cms",
    "astro",
    "nextjs",
    "typescript",
    "codegen",
    "types",
    "actions",
    "zod"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "clean": "turbo run clean && rm -rf node_modules",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "lint": "eslint packages/*/src/**/*.ts",
    "format": "prettier --write \"packages/*/src/**/*.ts\"",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@types/node": "^20.10.0",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "prettier": "^3.2.0",
    "tsup": "^8.0.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "license": "MIT",
  "author": "Eleven Estudio <hello@eleven-estudio.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/eleven-estudio/strapi2front.git"
  }
}